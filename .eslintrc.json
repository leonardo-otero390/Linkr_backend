{
  "env": {
      "browser": true,
      "es2021": true
  },
  "extends": [
      "airbnb-base",
      "prettier"
  ],
  "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
  },
  "rules": {
      "import/prefer-default-export": "off",
      "import/extensions": [
      "error",
      {
          "js": "ignorePackages"
      }
     ]
  },
  "overrides": [
        {
          "files": "*test.js",
          "rules": {
              "no-undef":"off"
          }
        }
  ]
}
